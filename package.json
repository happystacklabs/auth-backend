{
  "name": "@happystack/backend-boilerplate",
  "version": "0.2.2",
  "private": false,
  "author": "Heinrich Tremblay",
  "homepage": "https://boilerplate.happystack.io",
  "repository": "git+https://github.com/happystacklabs/backend-boilerplate.git",
  "bugs": {
    "url": "https://github.com/happystacklabs/backend-boilerplate/issues"
  },
  "keywords": [
    "api",
    "backend",
    "boilerplate",
    "express"
  ],
  "license": "MIT",
  "engines": {
    "node": "8.9.3"
  },
  "main": "app.js",
  "scripts": {
    "start": "node ./build/index.js",
    "dev": "nodemon --exec npm run babel-node ./src/index.js",
    "build": "babel ./src --out-dir ./build --copy-files",
    "babel-node": "babel-node --presets=env",
    "test": "jest --watch",
    "ci": "jest",
    "coverage": "jest --coverage && open ./coverage/lcov-report/src/index.html",
    "lint": "eslint src/**",
    "deploy": "runner deploy.sh",
    "heroku-postbuild": "npm run build"
  },
  "dependencies": {
    "babel-cli": "6.26.0",
    "babel-preset-env": "1.6.1",
    "bcrypt": "^1.0.3",
    "body-parser": "1.18.2",
    "cors": "2.8.4",
    "dotenv": "^4.0.0",
    "errorhandler": "1.5.0",
    "express": "4.16.2",
    "express-jwt": "5.3.0",
    "helmet": "3.9.0",
    "jsonwebtoken": "8.1.0",
    "mongoose": "4.13.7",
    "mongoose-unique-validator": "1.0.6",
    "morgan": "1.9.0",
    "passport": "0.4.0",
    "passport-local": "1.0.0",
    "throng": "4.0.0"
  },
  "devDependencies": {
    "eslint": "^4.14.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.5.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "jest": "22.0.3",
    "nodemon": "1.14.1",
    "rimraf": "2.6.2",
    "snyk": "^1.65.1",
    "supertest": "3.0.0"
  },
  "babel": {
    "presets": [
      "env"
    ]
  }
}
